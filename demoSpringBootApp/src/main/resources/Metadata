public class Metadata {
	private String NameofCamp;
	private String DescriptionofCamp;
	private String TypeofCamp;
	
	
/*costruttore*/
	public Metadata(String n, String d, String t) {
		this.NameofCamp = n;
		this.DescriptionofCamp = d;
		this.TypeofCamp = t;
	}
	
	
	
	/*funzioni di get e set per Nome del campo*/
	
	public String getNameofCamp() {
		return NameofCamp;
	}
	public void setNameofCamp(String NameofCamp) {
		this.NameofCamp = NameofCamp;
	}
	
	/*funzioni di get e set per descrizione del campo*/
	public String getDescriptionofCamp() {
		return DescriptionofCamp;
		}
	public void setDescriptionofCamp(String DescriptionofCamp) {
		this.DescriptionofCamp = DescriptionofCamp;
		}
	
	/*funzioni di get e set per tipo del campo*/
	public String getTypeofCamp() {
		return TypeofCamp;
	}
	public void setTypeofCamp(String TypeofCamp) {
		this.TypeofCamp = TypeofCamp;
	}
	
}
